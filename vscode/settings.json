{
    "editor.fontSize": 16,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "search.showLineNumbers": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.rulers": [
        80
    ],
    "vyper.analysis.onSave": false,
    "vyper.decoration.enable": false,
    "vyper.compile.onSave": false,
    "extensions.ignoreRecommendations": true,
    "workbench.editor.enablePreview": false,
    "telemetry.enableTelemetry": false,
    "files.watcherExclude": {
        "**/__pycache__/**": true,
        "**/.vscode/**": true
    },
    "search.exclude": {
        "**/__pycache__": true
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/.idea": true,
        "**/.vscode": true
    },
    "workbench.colorTheme": "Borealis",
    "editor.renderWhitespace": "none",
    "jupyter.searchForJupyter": false,
    "python.showStartPage": false,
    "jupyter.allowImportFromNotebook": false,
    "jupyter.allowInput": false,
    "jupyter.collapseCellInputCodeByDefault": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "python.experiments.enabled": false,
    "window.zoomLevel": 0,
    "python.pythonPath": "",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        },
        {
            "viewType": "jupyter-notebook",
            "filenamePattern": "*.ipynb"
        }
    ],
    "workbench.activityBar.visible": false
}